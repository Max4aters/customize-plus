{
  "name": "@knitkode/customize-plus",
  "version": "1.1.1",
  "description": "Enhance and extend the WordPress Customize in your themes.",
  "config": {
    "slug": "customize-plus",
    "title": "Customize Plus",
    "textdomain": "kkcp",
    "startYear": 2014,
    "svn": {
      "user": "knitkode"
    },
    "paths": {
      "app": "src",
      "distImages": "images",
      "distStyles": "css",
      "distScripts": "js",
      "distAssets": "../.tmp/wp-assets",
      "wpReleases": "../roots/releases/",
      "wpAssets": "../ui/customize-plus",
      "docsJsDest": "../knitkode.com/build/docs/customize-plus/advanced/javascript-api/jsdoc",
      "esdocDest": "../knitkode.com/build/docs/customize-plus/advanced/javascript-api/esdoc",
      "documentationDest": "../knitkode.com/build/docs/customize-plus/advanced/javascript-api/documentation-js",
      "documentationMarkdownDest": "../knitkode.com/src/content/docs/customize-plus"
    },
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "homepage": "https://knitkode.com/products/customize-plus",
  "author": {
    "name": "KnitKode",
    "email": "dev@knitkode.com",
    "url": "https://knitkode.com/"
  },
  "license": "SEE LICENSE IN license.txt",
  "private": false,
  "repository": {
    "url": "https://github.com/knitkode/customize-plus"
  },
  "bugs": {
    "url": "https://github.com/knitkode/customize-plus/issues",
    "email": "dev@knitkode.com"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "src/scripts",
    "src/styles",
    "src/images"
  ],
  "scripts": {
    "flow-typed-install": "flow-typed install --ignoreDeps dev",
    "prebuild": "rm -r build/ || true",
    "predeploy": "rm -r build/ || true",
    "prewatch": "yarn link @knitkode/vendor",
    "watch": "gulp watch --cwd config/",
    "build": "gulp build --dist --cwd config/",
    "deploy": "gulp deploy --dist --cwd config/",
    "publish": "gulp publish --dist --cwd config/",
    "update": "git submodule update --remote config/dev-lib && git add config/dev-lib && git commit -m \"maintenance(dev-lib) update\"",
    "init": "git submodule add git@github.com:knitkode/wp-dev-lib config/dev-lib && yarn link && yarn link @knitkode/vendor",
    "w": "npm run watch",
    "b": "npm run build",
    "d": "npm run deploy",
    "p": "npm run publish",
    "u": "npm run update",
    "i": "npm run init"
  },
  "dependencies": {
    "@knitkode/vendor": "github:knitkode/vendor.git",
    "classlist.js": "github:eligrey/classList.js.git",
    "css-toggle-switch": "4.1.x",
    "jQuery-ui-Slider-Pips": "github:simeydotme/jQuery-ui-Slider-Pips.git",
    "locutus": "2.0.x",
    "marked": "0.6.x",
    "selectize": "0.12.x",
    "spectrum-colorpicker": "1.8.x",
    "validator": "10.10.x"
  },
  "peerDependencies": {
    "jquery": "1.12.4",
    "modernizr": "3.6.0",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@wordpress/browserslist-config": "2.x.x",
    "autoprefixer": "9.4.x",
    "browserslist": "4.x.x",
    "clean-documentation-theme": "^0.5.2",
    "commitizen": "3.0.x",
    "css-mqpacker": "7.0.x",
    "cssnano": "4.x.x",
    "cz-conventional-changelog": "2.1.x",
    "deep-extend": "0.6.x",
    "del": "3.0.x",
    "documentation": "^9.1.1",
    "esdoc": "^1.1.0",
    "esdoc-flow-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "flow-bin": "^0.92.1",
    "gh-pages": "2.0.x",
    "grunt": "^1.0.3",
    "grunt-wp-deploy": "1.3.x",
    "gulp": "4.0.x",
    "gulp-base64": "github:govau/gulp-base64.git",
    "gulp-cache": "1.1.x",
    "gulp-checktextdomain": "2.2.x",
    "gulp-concat": "2.6.x",
    "gulp-data": "1.3.x",
    "gulp-debug": "4.0.x",
    "gulp-esdoc": "^0.4.1",
    "gulp-filelog": "0.4.x",
    "gulp-flatten": "0.4.x",
    "gulp-header": "2.0.x",
    "gulp-if": "2.0.x",
    "gulp-imagemin": "5.0.x",
    "gulp-include": "2.3.x",
    "gulp-jsdoc3": "2.0.x",
    "gulp-modernizr": "3.1.x",
    "gulp-postcss": "8.0.x",
    "gulp-potomo": "1.0.x",
    "gulp-readme-to-markdown": "0.2.x",
    "gulp-rename": "1.4.x",
    "gulp-replace": "1.0.x",
    "gulp-sass": "4.0.x",
    "gulp-shell": "0.6.x",
    "gulp-sourcemaps": "2.6.x",
    "gulp-strip-debug": "3.0.x",
    "gulp-terser": "1.x.x",
    "gulp-uglify": "3.0.x",
    "gulp-watch": "5.0.x",
    "gulp-wp-pot": "2.3.x",
    "gulp-zip": "4.2.x",
    "jsdoc-template": "^1.2.0",
    "lodash.template": "4.4.x",
    "minimist": "1.2.x",
    "node-wp-i18n": "1.2.x",
    "rollup": "1.1.x",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "9.2.x",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-node-resolve": "4.0.x",
    "streamqueue": "1.1.x"
  },
  "resolutions": {
    "@babel/core": "7.x.x"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ]
}
